{
    "name": "flutter-layout-builder-wrapper",
    "displayName": "Flutter LayoutBuilder Wrapper",
    "description": "Wrap selected widget with LayoutBuilder",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:dart"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.wrapWithLayoutBuilder",
                "title": "Wrap with LayoutBuilder"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "@types/node": "^14.x",
        "@typescript-eslint/eslint-plugin": "^4.x",
        "@typescript-eslint/parser": "^4.x",
        "eslint": "^7.x",
        "typescript": "^4.x"
    }
}