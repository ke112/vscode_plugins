{
    "name": "flutter-plugins-zhangzhihua",
    "displayName": "Flutter Plugins",
    "description": "提高Flutter开发效率，快速Wrapper代码生成、快速Build Runner、快速新建Getx页面等",
    "version": "0.0.13",
    "publisher": "zhangzhihua",
    "author": {
        "name": "徐凤年",
        "email": "zhangzhihua.email@gmail.com"
    },
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Snippets",
        "Debuggers",
        "Programming Languages",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:dart",
        "workspaceContains:**/pubspec.yaml"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^14.x",
        "@types/vscode": "^1.74.0",
        "@typescript-eslint/eslint-plugin": "^4.x",
        "@typescript-eslint/parser": "^4.x",
        "eslint": "^7.x",
        "typescript": "^4.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ke112/vscode_plugins.git"
    },
    "homepage": "https://github.com/ke112/vscode_plugins",
    "bugs": {
        "url": "https://github.com/ke112/vscode_plugins/issues"
    },
    "files": [
        "images",
        "out",
        "scripts"
    ],
    "contributes": {
        "configuration": {
            "title": "FNPlugin",
            "properties": {
                "fnplugin.logging.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable or disable logging for the FNPlugin extension."
                },
                "fnplugin.flutter.excludedWidgets": {
                    "type": "array",
                    "default": [
                        "Container",
                        "SizedBox",
                        "Padding",
                        "Row",
                        "Column"
                    ],
                    "description": "List of widget names to be excluded from the 'Wrap with...' commands."
                }
            }
        },
        "commands": [
            {
                "command": "extension.compressToWebP",
                "title": "★ Convert all images to webp"
            },
            {
                "command": "extension.createGetxBindingPage",
                "title": "★ Create getx binding page"
            },
            {
                "command": "extension.createGetxBasePage",
                "title": "★ Create getx and goRouter page"
            },
            {
                "command": "extension.buildRunner",
                "title": "★ Generate all codes"
            },
            {
                "command": "extension.generateIOSAppIcons",
                "title": "★ Generate all iOS logos"
            },
            {
                "command": "extension.buildRunnerQuick",
                "title": "★ Quickly generate .g.dart"
            },
            {
                "command": "extension.generateAssets",
                "title": "★ Quickly generate lib/gen (image/font)"
            },
            {
                "command": "extension.wrapWithAfterLayout",
                "title": "Wrap with AfterLayout"
            },
            {
                "command": "extension.wrapWithAnnotatedRegion",
                "title": "Wrap with AnnotatedRegion"
            },
            {
                "command": "extension.wrapWithAnimatedBuilder",
                "title": "Wrap with AnimatedBuilder"
            },
            {
                "command": "extension.wrapWithClipRRect",
                "title": "Wrap with ClipRRect"
            },
            {
                "command": "extension.wrapWithGestureDetector",
                "title": "Wrap with GestureDetector"
            },
            {
                "command": "extension.wrapWithInkWell",
                "title": "Wrap with InkWell"
            },
            {
                "command": "extension.wrapWithLayoutBuilder",
                "title": "Wrap with LayoutBuilder"
            },
            {
                "command": "extension.wrapWithMeasureSize",
                "title": "Wrap with MeasureSize"
            },
            {
                "command": "extension.wrapWithMediaQuery",
                "title": "Wrap with MediaQuery"
            },
            {
                "command": "extension.wrapWithObx",
                "title": "Wrap with Obx"
            },
            {
                "command": "extension.wrapWithPreferredSize",
                "title": "Wrap with PreferredSize"
            },
            {
                "command": "extension.wrapWithStack",
                "title": "Wrap with Stack"
            },
            {
                "command": "extension.wrapWithTheme",
                "title": "Wrap with Theme"
            },
            {
                "command": "extension.wrapWithValueListenableBuilder",
                "title": "Wrap with ValueListenableBuilder"
            },
            {
                "command": "extension.wrapWithValueListenableListBuilder",
                "title": "Wrap with ValueListenableListBuilder"
            },
            {
                "command": "extension.wrapWithVisibilityDetector",
                "title": "Wrap with VisibilityDetector"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "(resourceLangId == dart || explorerResourceIsFolder) && flutter-plugins.isFlutterProject",
                    "command": "extension.buildRunnerQuick",
                    "group": "navigation"
                },
                {
                    "when": "(resourceLangId == dart || explorerResourceIsFolder) && flutter-plugins.isFlutterProject",
                    "command": "extension.buildRunner",
                    "group": "navigation"
                },
                {
                    "when": "explorerResourceIsFolder && flutter-plugins.isFlutterProject",
                    "command": "extension.createGetxBindingPage",
                    "group": "navigation"
                },
                {
                    "when": "explorerResourceIsFolder && flutter-plugins.isFlutterProject",
                    "command": "extension.createGetxBasePage",
                    "group": "navigation"
                },
                {
                    "when": "flutter-plugins.isFlutterProject",
                    "command": "extension.generateIOSAppIcons",
                    "group": "navigation"
                },
                {
                    "when": "explorerResourceIsFolder && flutter-plugins.isFlutterProject",
                    "command": "extension.compressToWebP",
                    "group": "navigation"
                },
                {
                    "when": "flutter-plugins.isFlutterProject",
                    "command": "extension.generateAssets",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "when": "resourceLangId == dart",
                    "command": "editor.action.refactor",
                    "group": "1_modification"
                }
            ]
        }
    },
    "dependencies": {
        "image-size": "^1.1.1"
    }
}